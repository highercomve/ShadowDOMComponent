{
  "name": "shadow-component",
  "version": "0.2.0",
  "description": "This is a test implementation of components with shadow dom and full template rendering of changes of state",
  "main": "index.js",
  "scripts": {
    "build": "babel babel -d source",
    "watch": "babel -w babel -d source",
    "build-browser": "browserify source/index.js --standalone Shaco | uglifyjs -c > ./dist/shacho.min.js",
    "watch-browser": "watchify source/index.js -o ./dist/shaco.js --standalone Shaco -v",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/highercomve/shadow-component.git"
  },
  "keywords": [
    "shadow",
    "dom",
    "dom",
    "component"
  ],
  "author": "Sergio Marin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/highercomve/shadow-component/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "homepage": "https://github.com/highercomve/shadow-component#readme",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.2",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "uglifyjs": "^2.4.10",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "document-register-element": "^0.5.4",
    "incremental-dom": "^0.3.0"
  }
}
